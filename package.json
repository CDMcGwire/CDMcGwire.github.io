{
  "name": "cdmcgwire.github.io",
  "version": "2.1.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "deploy": "vite build && gh-pages -d build -b master -m 'Deployed from source branch' -t -r https://github.com/CDMcGwire/CDMcGwire.github.io.git",
    "ghHelp": "gh-pages --help"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@playwright/test": "^1.38.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "gh-pages": "^4.0.0",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.30",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rehype-add-classes": "^1.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^6.0.0",
    "sass": "^1.68.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/antic-didone": "^4.5.10",
    "@fontsource/baskervville": "^4.5.11",
    "@fontsource/jetbrains-mono": "^4.5.12"
  }
}
